{
  "manifest_version": 3,
  "name": "CryptoKitty Designer",
  "version": "1.0.0",
  "description": "Design your own CryptoKitties",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "icon.png"
    },
    "default_width": 800,
    "default_height": 600
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
    "web_accessible_resources": [{
    "resources": ["cattributes/*"],
    "matches": ["<all_urls>"]
  }]
}